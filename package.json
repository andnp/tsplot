{
    "scripts": {
        "build": "rm -rf dist && tsc",
        "test": "tsc && NODE_PATH=\"dist/src/\" ava",
        "versionBump": "ts-node scripts/versionBump.ts",
        "watch": "tsc --watch"
    },
    "pre-push": [
        "test",
        "versionBump"
    ],
    "dependencies": {
        "@types/lodash": "^4.14.74",
        "@types/node": "^8.0.30",
        "@types/plotly.js": "^1.28.5",
        "@types/puppeteer": "^1.8.0",
        "d3": "^3.5.17",
        "lodash": "^4.17.4",
        "plotly.js": "^1.29.3",
        "puppeteer": "^1.8.0",
        "utilities-ts": "git+https://github.com/andnp/utilities-ts.git"
    },
    "devDependencies": {
        "ava": "^0.25.0",
        "pre-push": "^0.1.1",
        "ts-node": "^7.0.0",
        "typescript": "^3.0.0"
    },
    "ava": {
        "timeout": "1m",
        "files": [
            "dist/tests/**/*.js"
        ],
        "source": [
            "dist/src/**/*.js"
        ]
    },
    "name": "tsplot",
    "version": "0.1.37",
    "description": "A server-side plotting utility currently focused around puppeteer and plotly",
    "main": "dist/src/index",
    "typings": "src/index",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andnp/tsplot.git"
    },
    "keywords": [
        "plotly",
        "nodejs"
    ],
    "author": "Andy Patterson",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/andnp/tsplot/issues"
    },
    "homepage": "https://github.com/andnp/tsplot#readme"
}